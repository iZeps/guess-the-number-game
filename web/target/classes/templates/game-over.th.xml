<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head::headFragment}"/>
    <attr sel="#game-title"
          th:text="#{game.title}"/>
    <attr sel="#game-over"
          th:text="#{game.over}"/>
    <attr sel="#resultMessage"
          th:text="${__${T(training.programming.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#playAgain"
          th:href="@{__${T(training.programming.util.GameMappings).RESTART}__}"
          th:text="#{game.play.again}"/>
    <attr sel="#homeId"
          th:href="@{__${T(training.programming.util.GameMappings).HOME}__}"
          th:text="#{game.home}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer::footerFragment}"/>
</thlogic>