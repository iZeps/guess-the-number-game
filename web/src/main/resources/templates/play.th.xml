<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head::headFragment}"/>
    <attr sel="#mainMessage"
          th:text="${__${T(training.programming.util.AttributeNames).MAIN_MESSAGE}__}"/>
    <attr sel="#resultMessage"
          th:text="${__${T(training.programming.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#formId"
          th:action="@{__${T(training.programming.util.GameMappings).PLAY}__}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer::footerFragment}"/>

    <attr sel="#game-title"
          th:text="#{game.title}"/>
    <attr sel="#enter-guess"
          th:text="#{game.enter.guess}"/>
    <attr sel="#submit"
          th:value="#{game.submit}"/>
</thlogic>